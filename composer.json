{
    "name": "jlorente/yii2-widget-datatables",
    "description": "A Yii2 extension to provide the framework with the JQuery DataTables plugin",
    "type": "yii2-extension",
    "minimum-stability": "stable",
    "keywords": ["yii2", "extension", "jquery", "datatables", "table", "javascript", "library"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/jlorente/yii2-widget-datatables/issues?state=open",
        "source": "https://github.com/jlorente/yii2-widget-datatables"
    },
    "authors": [
        {
            "name": "José Lorente Martín",
            "email": "jose.lorente.martin@gmail.com"
        },
        {
            "name": "Federico Nicolás Motta",
            "email": "fedemotta@gmail.com"
        }
    ],
    "require": {
        "bower-asset/datatables": ">= 1.9.4",
        "bower-asset/datatables-bootstrap3": "*",
        "bower-asset/datatables-tabletools": "*"
    },
    "autoload": {
        "psr-4": {
            "jlorente\\datatables\\": "src/"
        }
    }
}
